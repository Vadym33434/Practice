#include <stdio.h>

int main() {
    int t1, t2, t3;
    double total_time, average_time;

    // Зчитуємо вхідні дані
    scanf("%d %d %d", &t1, &t2, &t3);

    // Обчислюємо загальний час
    total_time = t1 + t2 + t3;

    // Обчислюємо середній час в годинах
    average_time = total_time / 3.0;

    // Виводимо результат з округленням до 2 знаків після коми
    printf("%.2f\n", average_time);

    return 0;
}
